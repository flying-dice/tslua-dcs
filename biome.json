{
	"$schema": "https://biomejs.dev/schemas/1.3.3/schema.json",
	"files": {
		"include": ["**/*.ts"],
		"ignore": ["node_modules", "docs"]
	},
	"vcs": {
		"enabled": true,
		"clientKind": "git",
		"useIgnoreFile": true
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"noForEach": "off"
			}
		}
	}
}
