{
	"name": "@flying-dice/tslua-http-api",
	"version": "0.1.2",
	"description": "LUA HTTP API Server written in Typescript Transpiled to lua using TypeScriptToLua",
	"keywords": [
		"lua",
		"http",
		"api"
	],
	"repository": {
		"type": "git",
		"url": "git@github.com:flying-dice/tslua-dcs.git"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "ISC",
	"types": "./dist/index.d.ts",
	"main": "./dist/index",
	"scripts": {
		"build": "rimraf dist && tsc -p tsconfig.tstl.json && tstl -p tsconfig.tstl.json",
		"dev": "cd dist && lua test-app.lua",
		"dev:watch": "nodemon",
		"build:docs": "typedoc",
		"test": "concurrently -k -s first \"npm run dev > .test-app.log\" \"vitest run\""
	},
	"nodemonConfig": {
		"verbose": true,
		"watch": [
			"dist"
		],
		"ext": "lua",
		"exec": "npm run dev"
	},
	"dependencies": {
		"@flying-dice/tslua-common": "^0.1.2",
		"@flying-dice/tslua-http": "^0.1.2",
		"@flying-dice/tslua-rxi-json": "^0.1.2"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^0.34.6",
		"axios": "^1.6.2",
		"lua-types": "^2.13.1",
		"typescript": "^5.3.2",
		"typescript-to-lua": "^1.22.0",
		"vitest": "^0.34.6",
		"vitest-mock-extended": "^1.3.1"
	},
	"gitHead": "2bcf7fe2ce2f467bdc9b03420a4672769bc3364c"
}
